{
  "name": "@onlyoffice/alfresco-extension",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "projects/onlyoffice-alfresco-extension/src",
  "prefix": "lib",
  "projectType": "library",
  "release": {
    "version": {
      "manifestRootsToUpdate": ["dist/{projectRoot}"],
      "currentVersionResolver": "git-tag",
      "fallbackCurrentVersionResolver": "disk"
    }
  },
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "projects/onlyoffice-alfresco-extension/ng-package.json",
        "tsConfig": "projects/onlyoffice-alfresco-extension/tsconfig.lib.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "projects/onlyoffice-alfresco-extension/tsconfig.lib.prod.json"
        },
        "development": {}
      },
      "defaultConfiguration": "production"
    },
    "nx-release-publish": {
      "executor": "@nx/js:release-publish",
      "dependsOn": [],
      "options": {
        "packageRoot": "dist/{projectRoot}",
        "publishDependencies": false
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
